{
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
    },
    "rules": {
        "strict": ["error", "never"],
        "valid-typeof":"error",
        "no-unexpected-multiline": "error",
        "no-unsafe-negation": "error",
        "no-unused-vars": "error",
        "no-undef": "error"
    },
    "env": {
        "browser": true
    }
    
}